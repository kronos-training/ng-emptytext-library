  "license": "MIT",
  "devDependencies": {
    "del": "^3.0.0",
    "node-sass": "^4.8.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-angular": "^0.5.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript": "^0.8.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "@angular/core": "^5.2.9",
    "rxjs": "^5.5.8"
  }
